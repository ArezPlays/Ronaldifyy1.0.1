{
  "expo": {
    "name": "Ronaldify",
    "slug": "ronaldify-5ml8ava",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rork-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0F0F1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.ronaldify-5ml8ava",
      "usesAppleSignIn": true,
      "buildNumber": "10",
      "infoPlist": {
        "NSCameraUsageDescription": "Ronaldify needs access to your camera to record training videos for AI analysis.",
        "NSPhotoLibraryUsageDescription": "Ronaldify needs access to your photo library to select training videos for AI analysis.",
        "NSMicrophoneUsageDescription": "Ronaldify needs access to your microphone to record audio with training videos.",
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.ronaldify_5ml8ava",
      "versionCode": 18,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "rork-app",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-apple-authentication",
      [
        "expo-image-picker",
        {
          "photosPermission": "Ronaldify needs access to your photos to select training videos.",
          "cameraPermission": "Ronaldify needs access to your camera to record training videos."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#00D084"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0F0F1A",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://rork.com/"
      },
      "eas": {
        "projectId": "873d52a1-2478-4d35-a369-9042bccb0d2d"
      }
    },
    "owner": "arezzz"
  }
}
