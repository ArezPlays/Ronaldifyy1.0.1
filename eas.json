{
  "cli": {
    "version": ">=16.32.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development"
      },
      "node": "20.19.4",
      "cli": {
        "installCommand": "npm install"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production"
      },
      "node": "20.19.4",
      "cli": {
        "installCommand": "npm install"
      }
    },
    "production": {
      "autoIncrement": false,
      "env": {
        "NODE_ENV": "production"
      },
      "node": "20.19.4",
      "cli": {
        "installCommand": "npm install"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
